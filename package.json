{
  "name": "tsangszechun.com",
  "description": "Personal website of Tsang Sze Chun",
  "version": "2.0.0",
  "author": "Jacky Tsang",
  "keywords": [
    "gatsby"
  ],
  "dependencies": {
    "gatsby": "^4.24.4",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-offline": "^5.24.0",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sitemap": "^5.24.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "scripts": {
    "dev": "gatsby develop -o",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "deploy": "gatsby build && echo tsangszechun.com > ./public/CNAME && gh-pages -d public -b master",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.12",
    "gatsby-plugin-postcss": "^5.24.0",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.8.3"
  }
}